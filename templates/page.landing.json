                  },
                  "block_order": [
                    "text_JHwtNJ"
                  ]
                },
                "accordion_row_m9MJrc": {
                  "type": "_accordion-row",
                  "settings": {
                    "heading": "Will it bounce when I run?",
                    "open_by_default": false,
                    "icon": "none",
                    "image_upload": "shopify://shop_images/Icon_Frame.png",
                    "width": 24
                  },
                  "blocks": {
                    "text_Dmbr7H": {
                      "type": "text",
                      "settings": {
                        "text": "<p>No. It was prototyped specifically to be bounce-free at speed and under fatigue.</p>",
                        "width": "100%",
                        "max_width": "none",
                        "alignment": "left",
                        "mobile_alignment": "left",
                        "type_preset": "rte",
                        "font": "var(--font-body--family)",
                        "font_size": "1rem",
