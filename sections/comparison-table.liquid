<style>
  .comparison-table {
    width: 100%;
    overflow-x: auto;
  }

  .comparison-table__wrapper {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    border: 1px solid rgba(var(--color-foreground-rgb), 0.1);
    border-radius: 0;
    overflow: hidden;
  }

  .comparison-table__column {
    display: flex;
    flex-direction: column;
    border-right: 1px solid rgba(var(--color-foreground-rgb), 0.1);
  }

  .comparison-table__column:last-child {
    border-right: none;
  }

  .comparison-table__column--featured {
    background-color: #000;
    color: var(--color-background);
  }

  .comparison-table__row {
    padding: 1rem;
    border-bottom: 1px dashed rgba(var(--color-foreground-rgb), 0.1);
    min-height: 60px;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    flex-direction: column;
  }

  .comparison-table__row--header {
    align-items: center;
    justify-content: center;
    text-align: center;
  }

  .comparison-table__row:not(.comparison-table__row--header) {
    flex-direction: row;
  }

  .comparison-table__column--featured .comparison-table__row {
    border-bottom-color: rgba(var(--color-background-rgb), 0.2);
  }

  .comparison-table__row:last-child {
    border-bottom: none;
  }

  .comparison-table__row-header {
    font-weight: 600;
  }

  .comparison-table__row-image {
    width: 100%;
    max-width: 150px;
    height: auto;
    margin-bottom: 0.5rem;
  }

  .comparison-table__row-product-name {
    font-weight: 600;
    margin-bottom: 0.5rem;
  }

  .comparison-table__row-data {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
  }

  .comparison-table__icon {
    width: 24px;
    height: 24px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .comparison-table__icon--check {
    width: 24px;
    height: 24px;
    border-radius: 50%;
    border: 2px solid currentColor;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    flex-shrink: 0;
  }

  .comparison-table__icon--check::after {
    content: '';
    width: 8px;
    height: 12px;
    border: solid currentColor;
    border-width: 0 2px 2px 0;
    transform: rotate(45deg);
    position: absolute;
    top: 3px;
  }

  .comparison-table__icon--x {
    width: 24px;
    height: 24px;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    flex-shrink: 0;
  }

  .comparison-table__icon--x::before,
  .comparison-table__icon--x::after {
    content: '';
    position: absolute;
    width: 18px;
    height: 2px;
    background-color: currentColor;
    transform: rotate(45deg);
  }

  .comparison-table__icon--x::after {
    transform: rotate(-45deg);
  }

  /* Mobile styles */
  @media (max-width: 767px) {
    .comparison-table {
      position: relative;
      -webkit-overflow-scrolling: touch;
    }

    .comparison-table__wrapper {
      display: flex;
      overflow-x: auto;
      scroll-snap-type: x mandatory;
      scroll-padding: 0;
    }

    .comparison-table__column {
      min-width: 200px;
      flex-shrink: 0;
      scroll-snap-align: start;
    }

    .comparison-table__column:first-child {
      position: sticky;
      left: 0;
      z-index: 10;
      background-color: var(--color-background);
      box-shadow: 2px 0 4px rgba(0, 0, 0, 0.1);
    }

    .comparison-table__column--featured:first-child {
      background-color: #000;
      box-shadow: 2px 0 4px rgba(0, 0, 0, 0.2);
    }
  }

  .comparison-table__wrapper {
    display: flex;
    /* justify-content: center; */
    flex-wrap: nowrap;
    max-width: 1200px;
    margin-left: auto;
    margin-right: auto;
    /* width: 100%; */
}

.comparison-table {
    display: flex;
}

.comparison-table__row.comparison-table__row--header {}

.comparison-table__column {
    min-width: 200px;
}

svg.icon-checkk path {
    fill: #000;
}

.comparison-table__column.comparison-table__column--featured {
    margin-top: -35px;
}

.comparison-table__column.comparison-table__column--featured svg.icon-checkk path{
    fill: #fff;
}

 .comparison-table__column.comparison-table__column--featured svg.icon-cross path {
    
    stroke: #fff;
}

.comparison-table__row {
    border-bottom: 1px dashed #0000003d!important;
    min-height: 70px;
}

 .comparison-table__column.comparison-table__column--featured .comparison-table__row {
    border-bottom: 1px dashed transparent;
}

.comparison-table__row-product-name {
    font-weight: 400;
    font-size: 20px;
}

.comparison-table {
    overflow: visible;
}

.comparison-table__wrapper {
    overflow: visible;
}

.comparison-table__row-image {
    height: 108px;
    margin-top: -80px;
}



