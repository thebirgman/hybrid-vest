    padding-left: 0;
}
.comparison-table__column {
    min-width: 120px;
    width: 120px;
}
}
.comparison-table {
    /* overflow-x: auto; */
}

.comparison-table__wrapper {
    overflow-x: auto;
    padding-top: 100px;
    padding-bottom: 20px;
}

.comparison-table__column.comparison-table__column--header {
    position: sticky;
    left: 0;
    z-index: 99;
    background: #fff;
}

.comparison-table__column.comparison-table__column--featured {
    position: sticky;
    left: 200px;
}

@media screen and (max-width: 768px) {
.comparison-table__column.comparison-table__column--featured {
    position: sticky;
    left: 120px;
}
}

.comparison-table__column.comparison-table__column--featured .comparison-table__row:last-child {
    padding-bottom: 40px;
    min-height: 90px;
}
@media screen and (max-width: 768px) {
