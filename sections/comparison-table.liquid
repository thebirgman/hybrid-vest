    font-size: 16px;
}

.comparison-table__column.comparison-table__column--featured {
    /* margin-top: -40px; */
}

.comparison-table__row.comparison-table__row-header {
    min-height: 90px;
}

.comparison-table__row.comparison-table__row--header {
    min-height: 80px;
}

.comparison-table__row {font-size: 14px;}

.comparison-table__row-data {
    text-align: center;
}

.comparison-table__column.comparison-table__column--header {
    box-shadow: none;
}
}

